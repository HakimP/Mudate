<p id="notice"><%= notice %></p>

<h1>Listing Solicituds</h1>
<%= params %>
<table>
  <thead>
    <tr>
      <th>Folio</th>
      <th>Fecha cita</th>
      <th>Fech inicio</th>
      <th>Fecha fin</th>
      <th>Fecha entrega caja</th>
      <th>Empresa servicio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solicituds.each do |solicitud| %>
      <tr>
        <td><%= solicitud.folio %></td>
        <td><%= solicitud.fecha_cita %></td>
        <td><%= solicitud.fech_inicio %></td>
        <td><%= solicitud.fecha_fin %></td>
        <td><%= solicitud.fecha_entrega_caja %></td>
        <td><%= solicitud.empresa_servicio %></td>
        <td><%= link_to 'Show', solicitud %></td>
        <td><%= link_to 'Edit', edit_solicitud_path(solicitud) %></td>
        <td><%= link_to 'Destroy', solicitud, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solicitud', new_solicitud_path %>
